        <script><![CDATA[var now = new GlideDateTime();
var tomorrow = new GlideDateTime();
tomorrow.addDaysLocalTime(1);

var table = new GlideRecord("x_58872_needit_needit_task");
table.addQuery("due_date", ">=", now);
table.addQuery("due_date", "<=", tomorrow);
table.addQuery("state", "!=", 3);
table.query();

while (table.next()) {
	gs.info("Task " + table.getValue("number") + " is due soon.");
	gs.eventQueue("x_58872_needit.dueSoonNeedItTasks", table, table.getValue("number"), gs.getUserName());
}
]]></script>
